<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>{{ pageTitle }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <link href="{{ app.request.baseUrl }}/Frontend/css/style.css" type="text/css" rel="stylesheet" media="screen" /> <!-- General style -->
    <link href="{{ app.request.baseUrl }}/Frontend/css/flexslider.css" type="text/css" rel="stylesheet" media="screen"> <!-- FlexSlider -->
    <link href="{{ app.request.baseUrl }}/Frontend/css/jcarousel.css" type="text/css" rel="stylesheet" media="screen" /> <!-- list_work -->
    <link rel="{{ app.request.baseUrl }}/Frontend/stylesheet" href="css/mosaic.css" type="text/css" media="screen" /><!-- mosaic -->
    <link href="{{ app.request.baseUrl }}/Frontend/css/prettyPhoto.css" type="text/css" rel="stylesheet" media="screen"><!-- prettyPhoto -->
    <link href="{{ app.request.baseUrl }}/Frontend/css/tipsy.css" type="text/css" rel="stylesheet" media="screen"><!--tooltip-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700|Arvo:400,400italic,700,700italic' rel='stylesheet' type='text/css'>
</head>

<body >
<div id="toTop">&uArr;</div>
<div id="page_wrap">
<h1 class="logo"><a href="{{ path('frontend.home') }}">lannimmoo</a></h1>
<div id="login-box" class="login-popup">
    <a href="#" class="close"><img src="{{ app.request.baseUrl }}/Frontend/images/process-stop.png" class="btn_close" title="Close Window" alt="Close" /></a>
    <form method="post" class="signin" action="#">
        <fieldset class="textbox">
            <label class="username">
                <span>Username or email</span>
                <input id="username" name="username" value="" type="text" autocomplete="on" placeholder="Username">
            </label>
            <label class="password">
                <span>Password</span>
                <input id="password" name="password" value="" type="password" placeholder="Password">
            </label>
            <button class="submit button" type="button">Sign in</button>
            <p>
                <a class="forgot" href="#">Forgot your password?</a> / <a class="register" href="#">Create an Account</a>
            </p>
        </fieldset>
    </form>
</div>
<span class="login"><a href="{{ path('auth.login') }}"> Login / Sign In</a></span>
<header>
    <nav id="smoothmenu1" class="ddsmoothmenu">
        <ul>
            <li><a href="{{ path('frontend.home') }}" class="current no-border-left">Home</a></li>
            <li><a href="{{ path('frontend.advertisements.overview') }}">Overzicht</a></li>
            <li><a href="{{ path('frontend.agency.overview') }}">Agentschappen</a></li>
        </ul>
    </nav>
</header>
    {% block pageContent %}
    {% endblock %}
<footer>
    <div id="footer_wrap">
        <div class="one-fourth first">
            <h3>Contact Information</h3>
            <p>Do you need help, support or advise?</p>
            <ul class="link">
                <li class="person">+1 3400 7000 123</li>
                <li class="mail">contact@sitename.com</li>
                <li class="locarea">9000 Cagayan de Oro, Philippines</li>
            </ul>
        </div>
        <div class="one-fourth">
            <h3>Properties</h3>
            <ul class="link">
                <li><a href="#">Commercial</a></li>
                <li><a href="#">Resedential</a></li>
                <li><a href="#">Luxury</a></li>
            </ul>
        </div>
    </div><!--end:#footer_wrap-->
    <div id="copy_right">
        <p>&copy; 2013. All Rights Reserved. Created by: <strong><a href="#">Lorenzo Lannoo</a></strong></p>
        <ul class="footer-menu">
            <li><a href="index-2.html">Home</a>/</li>
            <li><a href="about.html">Overzicht</a>/</li>
            <li><a href="contact.html">Contact</a></li>
        </ul>
    </div>
</footer><!--end:footer-->
</div><!--end:#page_wrap-->
{% block script %}

{% endblock %}

<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery-1.7.1.min.js"></script>

<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/css3-mediaqueries.js"></script><!--mediaqueries-->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/modernizr-1.7.min.js"></script><!--modernizr-->

<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/ddsmoothmenu.js"></script> <!--menu-->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.flexslider-min.js"></script> <!-- FlexSlider -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.jcarousel.min.js"></script> <!-- list_work -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/mosaic.1.0.1.js"></script> <!-- mosaic -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.easing.1.3.js"></script><!-- to work the quicksand -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.quicksand.js"></script><!-- quicksand -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.prettyPhoto.js"></script><!-- prettyPhoto -->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/jquery.tipsy.js"></script><!--tooltip-->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/organictabs.jquery.js"></script><!--tabs-->
<script type="text/javascript"src="{{ app.request.baseUrl }}/Frontend/js/responsivethumbnailgallery.js"></script><!--responsive gallery-->
<script type="text/javascript" src="{{ app.request.baseUrl }}/Frontend/js/custom.js"></script>
</body>
</html>
